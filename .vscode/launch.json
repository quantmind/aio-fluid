{
    "version": "0.2.0",
    "configurations": [

        {
            "name": "PyTest",
            "type": "python",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "console": "integratedTerminal",
            "module": "pytest",
            "justMyCode": false,
            "env": {},
            "args": [
                "-x",
                "tests/db/test_paginate.py"
            ],
            "debugOptions": [
                "RedirectOutput"
            ]
        },
        {
            "name": "AIO-FLUID EXAMPLE Serve",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/examples",
            "cwd": "${workspaceFolder}",
            "justMyCode": false,
            "args": [
                "serve"
            ],
            "debugOptions": [
                "RedirectOutput"
            ]
        },
        {
            "name": "AIO-FLUID EXAMPLE exec",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/examples",
            "cwd": "${workspaceFolder}",
            "justMyCode": false,
            "args": [
                "exec",
                "add",
                "--a",
                "1",
            ],
            "debugOptions": [
                "RedirectOutput"
            ]
        },
        {
            "name": "AIO-FLUID simple_worker",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/docs_src/simple_worker.py",
            "cwd": "${workspaceFolder}",
            "justMyCode": false,
            "debugOptions": [
                "RedirectOutput"
            ]
        },
    ]
}
